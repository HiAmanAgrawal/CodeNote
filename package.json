{
  "name": "codenote",
  "version": "0.1.0",
  "private": true,
  "description": "AI-Powered DSA Practice and Note-Taking Companion",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "db:generate": "prisma generate --schema=./db/schema.prisma",
    "db:push": "prisma db push --schema=./db/schema.prisma",
    "db:migrate": "prisma migrate dev --schema=./db/schema.prisma",
    "db:seed": "tsx db/seed.ts",
    "db:studio": "prisma studio --schema=./db/schema.prisma",
    "test:setup": "dotenv -e .env.local -- tsx scripts/test-setup.ts",
    "test:quick": "tsx scripts/quick-test.ts",
    "test:all": "npm run test:quick && npm run test:setup",
    "test-langchain": "tsx scripts/test-langchain.ts",
    "deploy": "bash scripts/deploy.sh"
  },
